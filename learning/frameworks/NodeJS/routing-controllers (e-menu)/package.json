{
  "name": "e-menu",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV='production' pm2-runtime start build_server -i 0",
    "dev": "run-p web server",
    "web": "react-scripts start",
    "server": "nodemon",
    "build": "npm-run-all build:clean build:web build:server build:static-pages",
    "build:clean": "node ./scripts/cleanBuild.js",
    "build:web": "react-scripts build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:static-pages": "node ./scripts/generateStatic.js",
    "test": "jest",
    "prod:logs": "heroku logs --app e-menu-amply --tail",
    "prod:stop-building": "heroku builds:cancel --app e-menu-amply",
    "pretty": "prettier --write '**/*.{ts,tsx,js,jsx,css,scss,json,md}'",
    "lint": "npx eslint './**/*.{ts,tsx,js,jsx}'",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "kill:server": "npx kill-port 3001",
    "kill:web": "npx kill-port 3000",
    "preinstall": "npx only-allow yarn"
  },
